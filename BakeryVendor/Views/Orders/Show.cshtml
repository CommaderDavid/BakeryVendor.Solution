@{
    Layout = "_Layout";
}
<h1>@Model["order"].Title Details: </h1>

<h2>Description: @Model["order"].Description</h2>
<h2>Price: $@Model["order"].Price</h2>
<h2>Date Ordered: @Model["order"].Date</h2>
<h3>Vendor: @Model["vendor"].Name</h3>

<a href='/vendors/@Model["vendor"].Id/orders/new'>Add another order</a> <br>
<a href='/vendors'>View all vendors</a>